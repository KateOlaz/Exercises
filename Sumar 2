#include <iostream>
using namespace std;

//Sums 4.3
int sum(const int p, const int q){
return p + q;
}

int sum(const int x, const int y, const int z){
return x + y + z;
}

int sum(const int a, const int b, const int c, const int d){
return a + b + c + d;
}

int main(){
cout<< sum(1,3,2,1)<<endl;
cout<< sum(2,6,3);

return 0;
}
